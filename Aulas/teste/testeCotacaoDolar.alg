algoritmo "semnome"
// Função :
// Autor :
// Data : 10/10/2023
// Seção de Declarações 
var
   valorGuardado, conversao : real
inicio
      escreva ("Quantos R$ voce tem guardado? ")
      leia (valorGuardado)
      conversao <- valorGuardado / 5.05
      escreval ("Você teria: ", conversao, " dólares")
fimalgoritmo
